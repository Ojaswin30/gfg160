class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        // code here
        int index=0,num=0;
        for(int i=0;i<arr.size(); i++){
            if(arr[i]==0){
                num+=1;
            }
            else{
                arr[index] = arr[i];
                index+=1;
            }
        }
        for(int i=index; i<index+num; i++){
            arr[i]=0;
        }
    }
};
